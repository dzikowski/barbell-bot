// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Price {
  id        String   @id @default(cuid())
  date      DateTime
  tokenIn   String
  amountIn  Float
  tokenOut  String
  amountOut Float
  fee       Float
}

model Trade {
  id            String   @id @default(cuid())
  date          DateTime
  uniqueId      String   @unique
  tokenIn       String
  amountIn      Float
  tokenOut      String
  amountOut     Float
  wasSuccessful Boolean
}
